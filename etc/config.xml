<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <gdpr>
            <general>
                <enabled>0</enabled>
            </general>
            <erasure>
                <enabled>0</enabled>
                <strategy>anonymize</strategy>
            </erasure>
            <anonymize>
                <customer_attributes></customer_attributes><!-- todo -->
                <customer_address_attributes></customer_address_attributes><!-- todo -->
            </anonymize>
            <export>
                <enabled>0</enabled>
                <customer_attributes></customer_attributes><!-- todo -->
                <customer_address_attributes></customer_address_attributes><!-- todo -->
            </export>
        </gdpr>
    </default>
</config>
